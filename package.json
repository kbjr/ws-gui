{
  "name": "ws-gui",
  "version": "0.0.9-alpha",
  "description": "An Electron based WebSocket client",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:mac": "electron-builder --mac",
    "dist:windows-linux": "electron-builder --win --linux",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kbjr/ws-gui.git"
  },
  "keywords": [
    "ws",
    "wss",
    "websocket",
    "socket",
    "gui",
    "electron",
    "client"
  ],
  "author": "James Brumond <james@jbrumond.me>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kbjr/ws-gui/issues"
  },
  "homepage": "https://github.com/kbjr/ws-gui#readme",
  "dependencies": {
    "prismjs": "^1.15.0",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "electron": "^3.0.0",
    "electron-builder": "^20.28.4",
    "eslint": "^5.6.0",
    "rimraf": "^2.6.2"
  },
  "build": {
    "appId": "com.electron.ws-gui",
    "compression": "maximum",
    "publish": [ "never" ],
    "win": {
      "target": [
        { "target": "nsis", "arch": [ "x64", "ia32" ] }
      ]
    },
    "mac": {
      "target": [
        { "target": "dmg", "arch": [ "x64", "ia32" ] }
      ]
    },
    "linux": {
      "category": "Development",
      "target": [
        { "target": "AppImage", "arch": [ "x64", "ia32" ] },
        { "target": "deb", "arch": [ "x64", "ia32" ]  },
        { "target": "rpm", "arch": [ "x64", "ia32" ]  },
        { "target": "freebsd", "arch": [ "x64", "ia32" ]  },
        { "target": "apk", "arch": [ "x64", "ia32" ]  }
      ]
    }
  }
}
